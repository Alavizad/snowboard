    "@babel/runtime": "^7.8.3",
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.3",
    "@oclif/plugin-not-found": "^1.2.3",
    "api-elements": "^0.2.2",
    "attributes-kit": "^1.0.4",
    "axios": "^0.19.2",
    "axios-auth-refresh": "^2.2.1",
    "axios-oauth-client": "^1.2.1",
    "baseui": "^9.44.0",
    "chalk": "^3.0.0",
    "chokidar": "^3.3.0",
    "cli-ux": "^5.4.2",
    "clipboard-copy": "^3.1.0",
    "commander": "^4.1.0",
    "cors": "^2.8.5",
    "cosmiconfig": "^6.0.0",
    "eidolon": "^1.6.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "fs-extra": "^8.1.0",
    "globby": "^11.0.0",
