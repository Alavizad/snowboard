    "prettier-plugin-svelte": "^0.7.0",
    "prop-types": "^15.7.2",
    "tape": "^4.13.0"
  },
  "browserslist": [
    "last 1 chrome versions"
  ],
  "keywords": [
    "api-blueprint"
  ]
}
