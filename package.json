    "commander": "^2.19.0",
    "express": "^4.16.4",
    "handlebars": "^4.1.2",
    "lodash": "^4.17.11",
    "marked": "^0.7.0",
    "mkdirp": "^0.5.1",
    "morgan": "^1.9.1",
    "outboard": "^2.7.1",
    "parse-duration": "^0.1.1",
    "parse-prefer-header": "^1.0.0",
    "prismjs": "^1.16.0",
    "remark": "^10.0.1",
    "rollup": "^1.16.7",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.1.0",
    "rollup-plugin-terser": "^5.1.1",
    "speakingurl": "^14.0.1",
    "svelte": "^3.6.5",
    "table": "^5.2.3",
    "tmp": "^0.1.0"
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "prettier": "^1.18.2",
    "prettier-plugin-svelte": "^0.6.0",
    "tape": "^4.10.1"
