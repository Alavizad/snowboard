<div class="ui horizontal divider">
  <a href="index.html#introduction">Introduction</a>
</div>
<div class="ui fluid secondary vertical menu">
  <a class="item" href="index.html#introduction">{{title}}</a>
</div>
{{#each resourceGroups}}
{{#if title}}
<div class="ui horizontal divider">
  {{#if title}}
    <a href="{{parameterize title}}.html#{{parameterize title}}">{{title}}</a>
  {{else}}
    <a href="index.html#{{parameterize title}}">{{title}}</a>
  {{/if}}
</div>
{{/if}}
<div class="ui accordion fluid">
  {{#each resources}}
    {{#if transitions}}
    {{#if title}}
    <div class="title {{#if @first}}active{{/if}}">
      <i class="dropdown icon"></i>
      <strong>{{title}}</strong>
    </div>
    {{/if}}
    <div class="content {{#if @first}}active{{/if}}">
      <div class="ui fluid secondary vertical menu">
      {{#each transitions}}
        {{#if ../../title}}
          <a class="item {{colorize method}}" href="{{parameterize ../../title}}.html#{{parameterize nestedTitle}}">
        {{else}}
          <a class="item {{colorize method}}" href="index.html#{{parameterize nestedTitle}}">
        {{/if}}
          <i class="ui {{colorize method}} empty circular label"></i>
          {{#if title}}
            <span>{{title}}</span>
          {{else}}
            <span>{{method}}</span>
          {{/if}}
        </a>
      {{/each}}
      </div>
    </div>
    {{/if}}
  {{/each}}
</div>
{{/each}}
