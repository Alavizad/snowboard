{
  "name": "snowboard",
  "version": "4.0.0-alpha1",
  "description": "API blueprint toolkit",
  "author": "Alif Rachmawadi <subosito@bukalapak.com>",
  "license": "MIT",
  "repository": "bukalapak/snowboard",
  "main": "lib/index.js",
  "bin": {
    "snowboard": "./bin/run"
  },
  "scripts": {
    "build-dev": "parcel build -t node --no-autoinstall --no-minify -d lib 'src/**/*.js'",
    "build": "parcel build -t node --no-source-maps --no-autoinstall -d lib 'src/**/*.js'",
    "fmt": "prettier --write README.md 'src/**/*.js' 'test/**/*.js' 'templates/**/*.{js,html,svelte}'",
    "test": "tape -r @babel/register 'test/**/*.js'"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "snowboard",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ]
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "dependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.3",
    "@oclif/plugin-not-found": "^1.2.3",
    "api-elements": "^0.2.2",
    "chalk": "^3.0.0",
    "chokidar": "^3.3.0",
    "cors": "^2.8.5",
    "cosmiconfig": "^6.0.0",
    "eidolon": "^1.6.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "fs-extra": "^8.1.0",
    "globby": "^11.0.0",
    "handlebars": "^4.7.2",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "ora": "^4.0.3",
    "parcel-bundler": "^1.12.4",
    "parse-prefer-header": "^1.0.0",
    "pretty-hrtime": "^1.0.3",
    "protagonist": "^2.0.2",
    "pure-uuid": "^1.6.0",
    "remark": "^11.0.2",
    "remark-parse": "^7.0.2",
    "speakingurl": "^14.0.1",
    "table": "^5.4.6",
    "tmp": "^0.1.0",
    "uritemplate": "^0.3.4"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@oclif/dev-cli": "^1.22.2",
    "prettier": "^1.18.2",
    "tape": "^4.13.0"
  },
  "browserslist": [
    "last 1 chrome versions"
  ],
  "keywords": [
    "api-blueprint"
  ]
}
